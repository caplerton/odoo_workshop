<odoo>
    <data>
    	<record id="view_feedback_tree" model="ir.ui.view">
            <field name="name">workshop.session.timeslot.tree</field>
            <field name="model">workshop.session.timeslot</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="name"/>
                    <field name="start_time"/>
                    <field name="end_time"/>
                </tree>
            </field>
        </record>

        <record id="view_timeslot_form" model="ir.ui.view">
            <field name="name">workshop.session.timeslot.form</field>
            <field name="model">workshop.session.timeslot</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <group>
                            <field name="name" readonly="True"/>
                            <field name="start_time"/>
                            <field name="end_time"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>
    </data>
</odoo>