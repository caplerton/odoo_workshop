<odoo>
    <data>
        <record id="view_session_tree" model="ir.ui.view">
            <field name="name">workshop.session.tree</field>
            <field name="model">workshop.session</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="name"/>
                    <field name="description"/>
                </tree>
            </field>
        </record>


        <record id="view_workshop_form" model="ir.ui.view">
            <field name="name">workshop.session.form</field>
            <field name="model">workshop.session</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>

                        <group>
                            <field name="name"/>
                            <field name="id"/>
                            <field name="description"/>
                            <!-- <field name="start_time" />
				            <field name="end_time" /> -->
                            <field name="time_ids" />
                            <field name="expert_id"/>
                            <field name="workshop_ids" />
							<field name="session_feedback_url" readonly="True"/>
							<field name="feedback_id" readonly="True"/>
							<field name="session_feedback_qr" widget="image" class="oe_avatar" width="500" height="500"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>
    </data>
</odoo>